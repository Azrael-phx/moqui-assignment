<?xml version="1.0" encoding="UTF-8"?>

<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                    xsi:noNamespaceSchemaLocation="http://www.moqui.org/xsd/entity-package-2.1.xsd">

    <entity entity-name="Example" package="example">
        <field name="exampleId" type="id" is-pk="true"/>
        <field name="exampleName" type="text-medium"/>
        <field name="examplePrice" type="number-decimal"/>
        <field name="exampleDate" type="date"/>
        <field name="exampleQuantity" type="number-integer"/>
    </entity>
</entities>